{
  "name": "electron-starter",
  "productName": "Electron Starter",
  "version": "0.0.1",
  "description": "A basic starter app for electron",
  "main": "build/index.js",
  "scripts": {
    "build": "gulp build",
    "clean": "gulp clean",
    "lint": "eslint ./src/ ./test/",
    "make:darwin": "gulp make:darwin",
    "make:deb": "gulp make:deb",
    "make:linux": "gulp make:linux",
    "make:rpm": "gulp make:rpm",
    "make:win": "gulp make:win",
    "package:darwin": "gulp package:darwin",
    "package:linux": "gulp package:linux",
    "package:win": "gulp package:win",
    "test": "npm run lint",
    "test-unit": "electron-mocha ./test -r test/support/prerun.js",
    "start": "electron . --dev",
    "watch": "gulp watch"
  },
  "author": {
    "name": "Samuel Attard",
    "email": "samuel.r.attard@gmail.com",
    "url": "https://www.samuelattard.com"
  },
  "homepage": "http://www.samuelattard.com",
  "license": "MIT",
  "dependencies": {
    "electron-prebuilt": "0.37.0",
    "lodash": "^4.6.1",
    "mkdirp": "^0.5.1",
    "yargs": "^4.2.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "chai": "^3.4.1",
    "electron-mocha": "^0.8.0",
    "electron-packager": "^5.2.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "grunt": "^0.4.5",
    "grunt-cli": "^0.1.13",
    "grunt-electron-installer": "^1.2.0",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.1",
    "gulp-clean": "^0.3.1",
    "gulp-concat": "^2.6.0",
    "gulp-cssmin": "^0.1.7",
    "gulp-electron": "0.0.10",
    "gulp-grunt": "^0.5.3",
    "gulp-less": "^3.0.5",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "material-design-icons-iconfont": "^2.0.4",
    "materialize-css": "^0.97.5",
    "mocha": "^2.3.4",
    "mocha-sinon": "^1.1.5",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0"
  },
  "optionalDependencies": {
    "electron-installer-debian": "^0.2.0",
    "electron-installer-redhat": "^0.2.0"
  }
}
